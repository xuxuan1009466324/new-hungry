<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.logo.entity">
<class name="User" table="tb_user">
	<id name="uId" column="uId">
		<generator class="native"></generator>
	</id>
	<property name="userNo" column="userNo"></property>
	<property name="userName" column="userName"></property>
	<property name="phone" column="phone"></property>
	<property name="email" column="email"></property>
	<property name="password" column="password"></property>
	<property name="registrationTime" column="registrationTime"></property>
	<property name="havePicture" column="havePicture"></property>
	<property name="status" column="status"></property>
</class>
</hibernate-mapping>
