<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

<class name="com.logo.entity.CancellationOrder" table="tb_cancellationOrder">
		<!-- 配置主键 -->
		<id name="cancellationOrderId" column="cancellationOrderId">
			<generator class="native"></generator>
		</id>
		<!-- 配置其他属性 -->
		<property name="cancellationOrderName" column="cancellationOrderName"></property>
		
	</class>

</hibernate-mapping>
